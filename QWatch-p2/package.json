{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "tailwindcss": {
    "config": "./tailwind.config.js"
  },
  "pnpm": {
    "hooks": {
      "preinstall": "node -r esm ./scripts/pnpm/preinstall.js",
      "postinstall": "node -r esm ./scripts/pnpm/postinstall.js"
    }
  },
  "devDependencies": {
    "pnpm": "^8.2.0"
  },
  "dependencies": {
    "@prisma/client": "4.13.0-dev.35",
    "autoprefixer": "^10.4.14",
    "next": "^13.3.0",
    "prisma": "4.13.0-dev.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1"
  }
}
